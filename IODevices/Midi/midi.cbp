<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
<CodeBlocks_project_file>
	<FileVersion major="1" minor="6" />
	<Project>
		<Option title="midi" />
		<Option pch_mode="0" />
		<Option compiler="gcc" />
		<Build>
			<Target title="debug">
				<Option output="../../../plugins/iodevices/midi_d.dll" prefix_auto="0" extension_auto="0" />
				<Option object_output=".wobjsd" />
				<Option deps_output=".depsd" />
				<Option type="3" />
				<Option compiler="gcc" />
				<Option createDefFile="1" />
				<Option createStaticLib="1" />
				<Compiler>
					<Add option="-g" />
				</Compiler>
				<Linker>
					<Add library="libopengl32" />
					<Add library="libglu32" />
					<Add library="libglaux" />
					<Add library="libmoldeod" />
				</Linker>
			</Target>
			<Target title="release">
				<Option output="../../../plugins/iodevices/midi.dll" prefix_auto="0" extension_auto="0" />
				<Option object_output=".wobjs" />
				<Option type="3" />
				<Option compiler="gcc" />
				<Option createDefFile="1" />
				<Option createStaticLib="1" />
				<Linker>
					<Add library="libopengl32" />
					<Add library="libglu32" />
					<Add library="libglaux" />
					<Add library="libmoldeo" />
				</Linker>
			</Target>
		</Build>
		<VirtualTargets>
			<Add alias="All" targets="debug;release;" />
		</VirtualTargets>
		<Compiler>
			<Add option="-DUNICODE" />
			<Add option="-DGLEW_STATIC" />
			<Add directory="./" />
			<Add directory="../../../include" />
			<Add directory="$(externlibs)/opengl/include" />
			<Add directory="$(externlibs)/lua/include" />
			<Add directory="$(externlibs)/glew/include" />
			<Add directory="$(externlibs)/portmidi/include" />
		</Compiler>
		<Linker>
			<Add library="portmidi_s" />
			<Add library="liblua" />
			<Add library="setupapi" />
			<Add library="winmm" />
			<Add library="user32" />
			<Add directory="../../../lib/win" />
			<Add directory="$(externlibs)/lua/lib" />
			<Add directory="$(externlibs)/cg/lib" />
			<Add directory="$(externlibs)/glew/lib" />
			<Add directory="$(externlibs)/portmidi/lib" />
		</Linker>
		<Unit filename="moMidi.cpp" />
		<Unit filename="moMidi.h" />
		<Extensions>
			<code_completion />
			<debugger />
			<envvars />
		</Extensions>
	</Project>
</CodeBlocks_project_file>
